http://3.91.253.207:8080/dptweb-1.0/

java 8
sudo apt update
sudo apt upgrade -y
sudo apt install openjdk-8-jdk -y
java -version

tomcat 8
sudo useradd -m -U -d /opt/tomcat -s /bin/false tomcat
cd /tmp
curl -O https://archive.apache.org/dist/tomcat/tomcat-8/v8.5.99/bin/apache-tomcat-8.5.99.tar.gz
sudo tar -xvzf apache-tomcat-8.5.99.tar.gz -C /opt/tomcat --strip-components=1
sudo chown -R tomcat:tomcat /opt/tomcat
sudo chmod +x /opt/tomcat/bin/*.sh

maven 3
sudo apt update
sudo apt upgrade -y
sudo apt install maven -y


clone the repo
git clone https://github.com/nagababuhyd/java-maven-tomcat/
   48  cd java-maven-tomcat/
   53  mvn package
   56  cd target
   67  cp -r dptweb-1.0 dptweb-1.0.war  /opt/tomcat/webapps/
cd /opt/tomcat/bin
./catalina.sh start

access application ip:8080/dptweb-1.0

   

